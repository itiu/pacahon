# imports: http://swrc.ontoware.org/ontology-07

@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix docs:   <http://gost19.org/docs#> .
@prefix gost19:  <http://gost19.org/base#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrc:    <http://swrc.ontoware.org/ontology#> .
@prefix user-onto:  <http://user-onto.org#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix zdb:     <http://user-db#> .

gost19:
      rdf:type owl:Ontology ;
      owl:imports <http://swrc.ontoware.org/ontology-07> ;
      owl:versionInfo "Created with TopBraid Composer"^^xsd:string .

gost19:Document
      rdf:type rdfs:Class ;
      rdfs:label "документ в понимании gost19"^^xsd:string ;
      rdfs:subClassOf swrc:Document ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:int ;
                owl:onProperty swrc:creationDate
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom xsd:date ;
                owl:onProperty swrc:creationDate
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:hasValue swrc:lastName ;
                owl:onProperty dc:creator
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:hasValue swrc:firstName ;
                owl:onProperty dc:creator
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:int ;
                owl:onProperty dc:creator
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom swrc:Person ;
                owl:onProperty dc:creator
              ] .

gost19:externalIdentifer
      rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf dc:identifier .

gost19:middlename
      rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf swrc:name .


swrc:Organization
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty swrc:name
              ] .

gost19:dateInterval
      rdf:type owl:DatatypeProperty ;
      rdfs:label "qqq"^^xsd:string ;
      rdfs:subPropertyOf swrc:date .

docs:contractor
      rdf:type rdf:Property ;
      rdfs:label "контрагент"^^xsd:string ;
      rdfs:subPropertyOf docs:attributeOfDocument .


